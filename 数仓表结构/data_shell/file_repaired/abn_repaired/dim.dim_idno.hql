insert overwrite table dim.dim_idno
select
idno_addr               ,
idno_area               ,
idno_area_cn            ,
idno_province           ,
case
when idno_province_cn='广西' then '广西壮族自治区'
when idno_province_cn='广西省' then '广西壮族自治区'
when idno_province_cn='重庆' then '重庆市'
when idno_province_cn='新疆' then '新疆维吾尔自治区'
when idno_province_cn='宁夏' then '宁夏回族自治区'
when idno_province_cn='西藏' then '西藏自治区'
when idno_province_cn='内蒙古' then '内蒙古自治区'
else idno_province_cn end as idno_province_cn,
idno_city               ,
idno_city_cn            ,
idno_county             ,
idno_county_cn
from dim.dim_idno;




